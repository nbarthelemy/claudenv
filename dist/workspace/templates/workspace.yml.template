# Workspace Configuration
# Generated by workspace installer on {timestamp}

organization:
  name: {org_name}
  github: {github_org}
  domain_suffix: "{domain_suffix}"
  dev_domain_suffix: "{dev_domain_suffix}"

defaults:
  stack: {default_stack}
  platform: {default_platform}

# Project registry
projects:
{projects}

# Port allocation
ports:
  start: {port_start}
  reserved:
    - 3001

# Shared packages (if applicable)
{packages_section}

# Infrastructure source
claude:
  source: github:nbarthelemy/workspace
  version: "{version}"
  base: github:nbarthelemy/claudenv
  baseVersion: "{claudenv_version}"
